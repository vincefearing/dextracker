{
  "name": "Dextracker Frontend",
  "dockerComposeFile": [
    "../../docker-compose.yml"
  ],
  "service": "frontend",
  "workspaceFolder": "/app",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "bradlc.vscode-tailwindcss",
        "mhutchie.git-graph"
      ]
    }
  },
  "postCreateCommand": "npm install"
}